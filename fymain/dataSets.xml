<?xml version="1.0" encoding="UTF-8"?>
<fydb>
    <dataSets>
		<!-- notice: all element names are case sensitive -->
        <!-- the "name" property should identify each dataSet and it CANNT be duplicated; it can be empty, the name will be generated by system in such scenario
		memType should be set and choose one of these values: HashMap, TreeMap
		phyType should be set and choose one of these values: DB_Oracle, DB_Mysql, DB_Mssql, DB_Sybase, DB_DB2, HashMap-->
		<!-- if phyType is DB_*, following nodes are required: tableName, keyColumns, connString, dbUser, dbPassword-->
        <dataSetDemo name="" memType="HashMap" phyType="DB_Oracle">
			<!-- table name -->
            <tableName>
                T_TEST1
            </tableName>
			<!-- Optional: where clause to query data from data source -->
			<where>
				OWNER = 'DEMO'
			</where>
			<!-- key columns -->
            <keyColumns>
                <column>
					OBJECT_ID
                </column>
            </keyColumns>
			<!-- JDBC connection string -->
            <connString>
                jdbc:oracle:thin:@huang:1521:edgar
            </connString>
			<!-- db user name -->
            <dbUser>
                demo
            </dbUser>
			<!-- db password -->
            <dbPassword>
                demo
            </dbPassword>
        </dataSetDemo>
        <dataSet name="" memType="HashMap" phyType="DB_Mysql">
			<!-- table name -->
            <tableName>
                T_user
            </tableName>
			<!-- key columns -->
            <keyColumns>
                <column>
					username
                </column>
            </keyColumns>
			<!-- JDBC connection string -->
            <connString>
                jdbc:mysql://huang2:3306/wapdb
            </connString>
			<!-- db user name -->
            <dbUser>
                wap
            </dbUser>
			<!-- db password -->
            <dbPassword>
                wap
            </dbPassword>
        </dataSet>
        <dataSet name="" memType="HashMap" phyType="DB_Mssql">
			<!-- table name -->
            <tableName>
                t_table
            </tableName>
			<!-- key columns -->
            <keyColumns>
                <column>
					name
                </column>
            </keyColumns>
			<!-- JDBC connection string -->
            <connString>
                jdbc:microsoft:sqlserver://huang2:1433;DatabaseName=sys
            </connString>
			<!-- db user name -->
            <dbUser>
                test
            </dbUser>
			<!-- db password -->
            <dbPassword>
                test
            </dbPassword>
        </dataSet>
        <dataSet name="" memType="HashMap" phyType="DB_Sybase">
			<!-- table name -->
            <tableName>
                t_table
            </tableName>
			<!-- key columns -->
            <keyColumns>
                <column>
					name
                </column>
            </keyColumns>
			<!-- JDBC connection string -->
            <connString>
                jdbc:sybase:Tds:localhost:2638
            </connString>
			<!-- db user name -->
            <dbUser>
                test
            </dbUser>
			<!-- db password -->
            <dbPassword>
                test
            </dbPassword>
        </dataSet>
        <dataSet name="" memType="HashMap" phyType="DB_DB2">
			<!-- table name -->
            <tableName>
                t_table
            </tableName>
			<!-- key columns -->
            <keyColumns>
                <column>
					name
                </column>
            </keyColumns>
			<!-- JDBC connection string -->
            <connString>
                jdbc:db2://localhost:50000/testdb
            </connString>
			<!-- db user name -->
            <dbUser>
                test
            </dbUser>
			<!-- db password -->
            <dbPassword>
                test
            </dbPassword>
        </dataSet>
        <dataSet name="" memType="HashMap" phyType="HashMap">
            <tableName>
                T_TEST2
            </tableName>
			<storeLocal>
				YES
			</storeLocal>
			<indexes>
				<indexO>
					<indexType>
						HashMap
					</indexType>
					<columns>
						<column>
							TABLESPACE_NAME
						</column>
						<column>
							NUM_ROWS
						</column>
					</columns>
				</indexO>
				<index>
					<indexType>
						TreeMap
					</indexType>
					<columns>
						<column>
							LAST_ANALYZED
						</column>
						<column>
							TABLE_NAME
						</column>
						<column>
							OWNER
						</column>
					</columns>
				</index>
			</indexes>
        </dataSet>
        <dataSet name="" memType="HashMap" phyType="HashMap">
            <tableName>
                T_USER
            </tableName>
            <keyColumns>
                <column>
					OWNER
                </column>
                <column>
					TABLE_NAME
                </column>
            </keyColumns>
			<indexes>
				<indexO>
					<indexType>
						HashMap
					</indexType>
					<columns>
						<column>
							TABLESPACE_NAME
						</column>
						<column>
							NUM_ROWS
						</column>
					</columns>
				</indexO>
				<index>
					<indexType>
						TreeMap
					</indexType>
					<columns>
						<column>
							LAST_ANALYZED
						</column>
						<column>
							TABLE_NAME
						</column>
						<column>
							OWNER
						</column>
					</columns>
				</index>
			</indexes>
        </dataSet>
        <dataSet name="" memType="HashMap" phyType="DB_Oracle">
            <tableName>
                T_TEST2
            </tableName>
			<!-- Optional: where clause to query data from data source -->
			<where>
				tablespace_name = 'RING'
			</where>
            <keyColumns>
                <column>
					OWNER
                </column>
                <column>
					TABLE_NAME
                </column>
            </keyColumns>
            <connString>
                jdbc:oracle:thin:@localhost:1521:edgar
            </connString>
            <dbUser>
                demo
            </dbUser>
            <dbPassword>
                demo
            </dbPassword>
			<indexes>
				<indexO>
					<indexType>
						HashMap
					</indexType>
					<columns>
						<column>
							TABLESPACE_NAME
						</column>
						<column>
							NUM_ROWS
						</column>
					</columns>
				</indexO>
				<index>
					<indexType>
						TreeMap
					</indexType>
					<columns>
						<column>
							LAST_ANALYZED
						</column>
						<column>
							TABLE_NAME
						</column>
						<column>
							OWNER
						</column>
					</columns>
				</index>
			</indexes>
        </dataSet>
    </dataSets>
</fydb>