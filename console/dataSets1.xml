<?xml version="1.0" encoding="GBK"?>
<fydb>
    <dataSets>
        <!-- the "name" property should identify each dataSet and it CANNT be duplicated; it can be empty, the name will be generated by system in such scenario
		memType should be set and choose one of these values: HashMap, SortedMap, TreeMap
		phyType should be set and choose one of these values: DB_Oracle, DB_Mysql, HashMap, SortedMap, TreeMap, File_Json, File_XML-->
		<!-- if phyType is DB_Oracle or DB_Mysql, following nodes are required: tableName, keyColumns, connString, dbUser, dbPassword-->
        <dataSet name="" memType="HashMap" phyType="DB_Oracle">
			<!-- table name -->
            <tableName>
                T_TEST1
            </tableName>
			<!-- key columns -->
            <keyColumns>
                <column>
					OWNER
                </column>
                <column>
					OBJECT_NAME
                </column>
            </keyColumns>
			<!-- JDBC connection string -->
            <connString>
                jdbc:oracle:thin:@huanged:1521:edgar
            </connString>
			<!-- db user name -->
            <dbUser>
                demo
            </dbUser>
			<!-- db password -->
            <dbPassword>
                demo
            </dbPassword>
        </dataSet>
    </dataSets>
</fydb>